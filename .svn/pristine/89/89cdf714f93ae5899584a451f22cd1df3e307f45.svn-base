<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kim.test.dao.MemoDao">
    <select id="selectAll" resultType="com.kim.test.entity.MemoEntity">
        select STAFF_CODE as staffCode
        from memo
        where staff_code = '00164'
    </select>
</mapper>